{
	"name": "Banner Schedule Exporter",
	"description": "Extension that exports your class schedule from Banner. Created by Abel Weldaregay and Chase Hartsell.",
	"version": "1.0",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"permissions": [
		"activeTab",
		"tabs"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "quake-way0l@icloud.com"
		}
	}
}